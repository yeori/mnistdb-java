# 4 FILES of MNIST DATABASE
# REQUIRED : true
#
# four files are located at
# 
#  http://yann.lecun.com/exdb/mnist/
#
# either one of two type(training, test), or both
# 
path.training.label=e:/mnist/train-labels.idx1-ubyte
path.training.img=e:/mnist/train-images.idx3-ubyte
#path.test.label=e:/mnist/t10k-labels-idx1-ubyte.gz
#path.test.img=e:/mnist/t10k-images-idx3-ubyte.gz

# OUTPUT DIRECTORY
# REQUIRED : true
#
out.dir=e:/mnist/raw

# OUTPUT MODE
# REQUIRED : false
# DEFAULT  : raw
# 
# one of raw, png, jpg
# 
#  * raw - writes raw bytes
#  * png - writes as png images
#  * jpg - writes as jpg images
#
out.mode=raw

# FILENAME PREFIX FOR TRAINING
# REQUIRED : false
# DEFAULT  : 'tr-'
#
# if prefix is 'tr-' file name would be
#
#   tr-00000-5.png
#   tr-00001-0.png
#   ...
#   tr-59999-9.png
#
out.training.prefix=tr-

# FILENAME PREFIX FOR TEST
# REQUIRED : false
# DEFAULT  : 'test-'
#
# if prefix is 'test-', then file name would be
#
#   test-0000-5.png
#   test-0001-0.png
#   ...
#   test-9999-4.png
#
out.test.prefix=test-
